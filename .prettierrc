{
  "arrowParens": "always",
  "endOfLine": "auto",
  "singleQuote": false,
  "trailingComma": "es5",
  "useTabs": false,
  "bracketSpacing": true,
  "tabWidth": 2,
  "printWidth": 90,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(react-(.*)$)",
    "<THIRD_PARTY_MODULES>",
    "^@/(.*)$",
    "^../(.*)$",
    "^./(.*)$",
    "^@heroicons/(.*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
